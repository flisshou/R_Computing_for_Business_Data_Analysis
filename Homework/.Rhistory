f = function(x) {
y = 0
if (x > 1) {
y = sqrt(x)
} else if (x <= 0) {
y = (-1) * x^3
} else{
y = x^2
}
y
}
datas = seq(-2, 2, 0.1)
results = rep(0, length(datas))
for (i in 1:length(datas)){
results[i] = f(datas[i])
}
plot(datas, results, type = 'l', xlab = 'x', ylab = 'f(x)')
